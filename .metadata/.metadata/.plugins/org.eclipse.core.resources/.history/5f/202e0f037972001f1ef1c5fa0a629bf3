
public class Test {

	/**
	 * @param args
	 */
	public static void main(String[] args) { 
		// TODO Auto-generated method stub
			int x=3;		
			if(x>=90){
				System.out.println ("A");
			}	
			else if(x>=80||x==89) {
				System.out.println ("B");
			}
			else if(x>=70||x==79) {
				System.out.println("C");
			}
			else {
				System.out.println ("F");
			}
	}

}
