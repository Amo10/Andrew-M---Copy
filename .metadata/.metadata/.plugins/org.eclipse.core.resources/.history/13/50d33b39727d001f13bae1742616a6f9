import java.lang.*;
public class BalloonProject {
	public static void main(String[] args){
		Balloon myBalloon = new Balloon();
		myBalloon.setColor("red");
		myBalloon.inflate(101);
		System.out.println("your " + (myBalloon.color) + " balloon has " + myBalloon.air + " molecules of air");
	}
}
